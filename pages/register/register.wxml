<back></back>
<view class="login">
  <view class="title">
  <view class="msg hello">在这里,</view>
  <view class="msg">开始你的旅程</view>
  </view>
  <view class="info">
    <input type="text" placeholder="请输入手机号" value="{{username}}" bindinput="changeUserName"/>
    <input type="password" placeholder="请输入密码" value="{{password}}" bindinput="changePassWord"/>
    <input type="password" placeholder="请再次输入密码" value="{{checkpassword}}" bindinput="changeCheckPassWord"/>
  </view>
  <view class="registerbtn">
    <button style="color: white; background-color: red; width: 90vw; border-radius: 100rpx;" bindtap="register"
    >注册</button>
  </view>
</view>